        <Button x:Name="runTest_button" Content="Run Test" HorizontalAlignment="Left" Margin="333,341,0,0" VerticalAlignment="Top" Width="75" Click="runTest_button_Click"/>
        <ComboBox x:Name="testList_comboBox" HorizontalAlignment="Left" Margin="85,339,0,0" VerticalAlignment="Top" Width="150"/>

    </Grid>
